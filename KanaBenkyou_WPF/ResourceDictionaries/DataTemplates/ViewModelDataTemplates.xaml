<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HerrNyani.KanaBenkyou_WPF.ResourceDictionaries.DataTemplates"
                    xmlns:views="clr-namespace:HerrNyani.KanaBenkyou_WPF.UserControls.Views"
                    xmlns:viewmodels="clr-namespace:HerrNyani.KanaBenkyou_WPF.ViewModels"
                    xmlns:consultingViewmodels="clr-namespace:HerrNyani.KanaBenkyou_WPF.ViewModels.Consulting"
                    xmlns:practisingViewmodels="clr-namespace:HerrNyani.KanaBenkyou_WPF.ViewModels.Practising"
                    xmlns:consultingViews="clr-namespace:HerrNyani.KanaBenkyou_WPF.UserControls.Views.Consulting"
                    xmlns:practisingViews="clr-namespace:HerrNyani.KanaBenkyou_WPF.UserControls.Views.Practising">

    <DataTemplate DataType="{x:Type viewmodels:StartPageViewModel}">
        <views:StartPageView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type consultingViewmodels:KanaChartViewModel}">
        <consultingViews:KanaChartView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type practisingViewmodels:KanaQuizViewModel}">
        <practisingViews:KanaQuizView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type consultingViewmodels:KanaDetailViewModel}">
        <consultingViews:KanaDetailView />
    </DataTemplate>
    
</ResourceDictionary>